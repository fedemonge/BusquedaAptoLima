{
  "searchResults": {
    "listingCard": "article.snippet",
    "listingUrl": "a.title, [data-test=\"snippet__title\"]",
    "nextPage": "#pagination-next[data-islast=\"false\"]"
  },
  "listingPage": {
    "title": "a.title, h1.title, [data-test=\"snippet__title\"]",
    "price": "[data-test=\"snippet__price\"], div.price",
    "squareMeters": "[data-test=\"area-value\"], span.properties__area",
    "bedrooms": "[data-test=\"bedrooms-value\"], span.properties__bedrooms",
    "bathrooms": "[data-test=\"full-bathrooms-value\"], span.properties__bathrooms",
    "parking": "[data-test=\"principal-amenity-value\"], span.properties__amenity__car_park",
    "neighborhood": "[data-test=\"snippet__location\"], div.location",
    "imageUrl": ".snippet__image img, .swiper-slide img"
  },
  "urlPatterns": {
    "searchBase": "/s",
    "listingBase": "/detalle"
  }
}
